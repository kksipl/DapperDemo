@model DapperDemo.Model.Tenant
<div style="padding:20px;">
    <div>
        <div class="card" style="width:500px">
            <div class="card-body">
                <h4 class="card-title">
                    @($"{Model.FirstName} {Model.MiddleName} {Model.LastName}")
                </h4>
                <i class="card-text">@($"{Model.ContactNumber1}")</i>
                <i class="card-text">@($"{Model.ContactNumber2}")</i>
            </div>
        </div>
    </div>
    <div>
        @Html.ActionLink("Edit", "Edit", new { id = Model.TenantId }) |
        <a asp-action="Index">Back to List</a> |
        <a asp-action="Delete" asp-route-id="@(Model.TenantId)" class="text-danger"><i class="fas fa-trash text-danger"></i> Delete</a>
    </div>
</div>